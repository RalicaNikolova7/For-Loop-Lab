using System;
using System.IO;

namespace SumNumbers
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int number = int.Parse(Console.ReadLine());
            int sum = 0;

            for (int i = 1; i <= number; i++)
            {
                int addNum = int.Parse(Console.ReadLine());
                sum += addNum;
            }

            Console.WriteLine(sum);


        }
    }
}

